{
	"name": "cfxcli-commands",
	"displayName": "cfxcli-commands",
	"description": "cfxcli-commands",
	"version": "0.0.1",
	"publisher": "Pitu7944",
	"repository": "",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.restartCurrentScript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cfxcli.restartCurrentScript",
				"title": "cfxcli: Restart Current Script"
			},
			{
				"command": "cfxcli.stopCurrentScript",
				"title": "cfxcli: Stop Current Script"
			},
			{
				"command": "cfxcli.startCurrentScript",
				"title": "cfxcli: Start Current Script"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/request": "^2.48.8",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"esbuild": "^0.14.21",
		"eslint": "^7.21.0",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"axios": "^0.26.0"
	}
}
